<section id="anggota" class="bg-white p-5 md:p-20 h-full mt-28 mb-28">
    <div class="mx-auto max-w-7xl px-4 lg:px-12">
        <h2 class="text-2xl font-bold mb-4 text-center">Anggota Kelas 01SIFP010</h2>
        <div class="bg-white relative shadow-md sm:rounded-lg overflow-hidden mt-10">
            <div class="overflow-x-auto">
                <table class="w-full text-sm text-left text-white">
                    <thead class="text-lg  uppercase bg-gray-500 text-white">
                        <tr>
                            <th scope="col" class="px-4 py-3">No</th>
                            <th scope="col" class="px-4 py-3">Nama Anggota</th>
                            <th scope="col" class="px-4 py-3">Link Website</th>
                            <th scope="col" class="px-4 py-3">PDF</th>
                        </tr>
                    </thead> 
                    <tbody>
                        {members.map((member) => (
                            <tr class="border-b border-gray-700">
                                <td class="px-4 py-3 text-gray-900">{member.id}</td>
                                <th scope="row" class="px-4 py-3 font-medium text-gray-900 whitespace-nowrap">{member.name}</th>
                                <td class="px-4 py-3 text-gray-900">
                                    <a href={`https://${member.subdomain}.kelas10sifp.my.id`} target="_blank" class="hover:underline">
                                        {`https://${member.subdomain}.kelas10sifp.my.id`}
                                    </a>
                                </td>
                                <td class="px-4 py-3 text-gray-900">
                                    <a href={member.pdf} target="_blank" class="hover:underline">
                                        Link PDF
                                    </a>
                                </td>
                            </tr>
                        ))}
                    </tbody>
                </table>
            </div>
        </div>
    </div>
    </section>